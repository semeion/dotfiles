[sendemail]
  smtpserver = /usr/sbin/dma

[alias]
  all = !git-all
  br = branch
  co = checkout
  last = show --stat HEAD
  logdel = log --diff-filter=D --summary
  out = cherry -v
  outt = cherry -v origin/master
  st = status

[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
  grep = yes

[merge]
	tool = vimdiff
	log = true

[push]
	default = current

[help]
	autocorrect = 1

[branch "master"]
	remote = origin
	merge = master

